/*16)
Console.WriteLine("Dati 5 numere de la tastatura:");
int z = int.Parse(Console.ReadLine());
int x = int.Parse(Console.ReadLine());
int c = int.Parse(Console.ReadLine());
int v = int.Parse(Console.ReadLine());
int b = int.Parse(Console.ReadLine());

if (z > x) { int t = z; z = x; x = t; }
if (z > c) { int t = z; z = c; c = t; }
if (z > v) { int t = z; z = v; v = t; }
if (z > b) { int t = z; z = b; b = t; }

if (x > c) { int t = x; x = c; c = t; }
if (x > v) { int t = x; x = v; v = t; }
if (x > b) { int t = x; x = b; b = t; }

if (c > v) { int t = c; c = v; v = t; }
if (c > b) { int t = c; c = b; b = t; }

if (v > b) { int t = v; v = b; b = t; }

Console.WriteLine($"Numerele vor fi in oridnea: {z}, {x}, {c}, {v}, {b}");*/

/*17)
Console.WriteLine("Dati doua numere:");
int a = Convert.ToInt32(Console.ReadLine());
int b = Convert.ToInt32(Console.ReadLine());

Console.WriteLine($"Cel mai mare divizor comun al numerelor {a} si {b} este {cmmdc(a,b)} si cel mai mic multiplu comun este {cmmmc(a,b)}");
static long cmmdc(long prim, long doi)
{
    while (doi != 0)
    {
        long restul = prim % doi;
        prim = doi;
        doi = restul;
    }
    return prim;
}
static long cmmmc(int primul , int doilea)
{
    long rezulat=(primul * doilea)/ cmmdc(primul, doilea);
    return rezulat;
}*/

/*18)
Console.WriteLine("Dati un numar pentru ai afla divizorii.");
int n=Convert.ToInt32(Console.ReadLine());

for (int i=2; i*i<=n; i++)
{
    int put = 0;
    while (n % i == 0)
    {
        put++;
        n = n / i;
    }
    if (put>0)
    {
        Console.WriteLine($"{i}^{put}");
    }
}
if (n>0)
{
    Console.WriteLine($"{n}^1");
}*/

/*19)
Console.WriteLine("Dati cifra la verificare:");
int z=Convert.ToInt32(Console.ReadLine());
int primtip = -1;
int doitip = -1;
bool gasit=true;
while (z > 0)
{
    int tip = z % 10;
    z=z/10;
    if (primtip==-1)
    {
        primtip = tip;
    }
    else if (doitip==-1 && tip!=primtip)
    {
        doitip = tip;
    }
    else if(tip != primtip && tip != doitip)
    {
        gasit = false;
        Console.WriteLine("Numarul nu este format din 2 cifre care se repeta.");
        break;
    }
}
if (gasit)
{
    Console.WriteLine("Numarul este format din 2 cifre care se repeta.");
}*/

