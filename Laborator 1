/*21)
int min = 1;
int max = 1024;
int incercari = 0;
int intrebari = 0;
while (min<= max)
{
    int x = (min + max) / 2;
    Console.WriteLine($"Numarul este mai mare ca {x} ? d/n ");
    string dsin=Console.ReadLine();
    intrebari++;
    if (dsin == "d")
    {
        min = x+1;
    }
    else
    {
        max=x-1;
    }
}

Console.WriteLine($"Numarul tau este {min} ghicit din {intrebari} intrebari");*/
