/*2)
Console.WriteLine("Ecuatia de gradul 2: ax^2 + bx + c = 0");
Console.Write("Introduceti coeficientul a: ");
double a = Convert.ToDouble(Console.ReadLine());
Console.Write("Introduceti coeficientul b: ");
double b = Convert.ToDouble(Console.ReadLine());
Console.Write("Introduceti coeficientul c: ");
double c = Convert.ToDouble(Console.ReadLine());

double delta = b*b - 4 * a *c;
if (delta > 0)
{
    double x1 = (-b + Math.Sqrt(delta)) / (2 * a);
    double x2 = (-b - Math.Sqrt(delta)) / (2 * a);
    Console.WriteLine($"Ecutatia data are solutiile x1 = {x1} si x2 = {x2}");
}
else if (delta == 0)
{
    double x = -b / (2 * a);
    Console.WriteLine($"Ecuatia are o singura solutie reala care este {x}");
}
else
{
    Console.WriteLine("Ecuatia nu are solutii reale.");
}*/


