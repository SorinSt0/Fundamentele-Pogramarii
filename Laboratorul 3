/*23)
Console.WriteLine("Cate elemente sa aiba primul vector: ");
int n1 = Convert.ToInt32(Console.ReadLine());
Console.WriteLine("introduceti vectorii in ordine strict crescatoare:");
int[] v1 = new int[n1];

for (int i = 0; i < v1.Length; i++)
{
    v1[i] = Convert.ToInt32(Console.ReadLine());
}
Console.WriteLine("Cate elemente sa aiba al doilea vector: ");
int n2 = Convert.ToInt32(Console.ReadLine());
Console.WriteLine("introduceti vectorii in ordine strict crescatoare:");
int[] v2 = new int[n2];

for (int i = 0; i < v2.Length; i++)
{
    v2[i] = Convert.ToInt32(Console.ReadLine());
}
int a1 = 0;
int b1 = 0;
int k1 = 0;

Console.Write("Intersectie:  ");
int[] inters = new int[Math.Min(n1, n2)];
while(a1<n1 && b1< n2)
{
    if (v1[a1] < v2[b1])
    {
        a1++;
    }
    else if (v1[a1] > v2[b1])
    {
        b1++;
    }
    else
    {
        inters[k1] = v1[a1];
        a1++; b1++; k1++;
    }
}

for(int i = 0; i < k1; i++)
{
    Console.Write(inters[i]+",");
}

int a2 = 0;
int b2 = 0;
int k2 = 0;
Console.WriteLine();
Console.Write("Reuniune:  ");
int comb = n1 + n2;
int[] reun = new int[comb];
while(a2<n1 && b2 < n2)
{
    if (v1[a2] < v2[b2])
    {
        reun[k2] = v1[a2];
        a2++; k2++;
    }
    else if(v1[a2] > v2[b2])
    {
        reun[k2] = v2[b2];
        b2++; k2++;
    }
    else
    {
        reun[k2]=v1[a2];
        a2++; k2++; b2++;
    }
}
while (a2 < n1)
    {
        reun[k2] = v1[a2];
        a2++; k2++;
    }
    while(b2 < n2)
    {
        reun[k2]= v2[b2];
        b2++; k2++;
    }
for (int i=0;i<k2;i++){
    Console.Write(reun[i]+",");
}

