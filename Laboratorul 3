/*26)
class Program
{
    static void Main()
    {

        Console.Write("Numar 1: ");
        string s1 = Console.ReadLine();
        Console.Write("Numar 2: ");
        string s2 = Console.ReadLine();

        List<int> n1 = StringToVector(s1);
        List<int> n2 = StringToVector(s2);

        List<int> suma = Adunare(n1, n2);
        Console.Write("Suma: ");
        AfisareVector(suma);

        List<int> dif = Scadere(n1, n2);
        Console.Write("Diferenta: ");
        AfisareVector(dif);

        List<int> prod = Inmultire(n1, n2);
        Console.Write("Produs: ");
        AfisareVector(prod);
    }
    static List<int> StringToVector(string s)
    {
        List<int> v = new List<int>();
        for (int i = s.Length - 1; i >= 0; i--) 
        {
            v.Add(s[i] - '0');
        }
        return v;
    }
    static void AfisareVector(List<int> v)
    {
        if (v.Count == 0) { Console.WriteLine("0"); return; }
        for (int i = v.Count - 1; i >= 0; i--)
        {
            Console.Write(v[i]);
        }
        Console.WriteLine();
    }
    static List<int> Adunare(List<int> a, List<int> b)
    {
        List<int> rez = new List<int>();
        int i = 0, j = 0, transport = 0;

        while (i < a.Count || j < b.Count || transport > 0)
        {
            int cifraA = (i < a.Count) ? a[i] : 0;
            int cifraB = (j < b.Count) ? b[j] : 0;

            int suma = cifraA + cifraB + transport;

            rez.Add(suma % 10);
            transport = suma / 10;
            i++; j++;
        }
        return rez;
    }
    static List<int> Scadere(List<int> a, List<int> b)
    {
        List<int> rez = new List<int>();
        int imprumut = 0;

        for (int i = 0; i < a.Count; i++)
        {
            int cifraA = a[i];
            int cifraB = (i < b.Count) ? b[i] : 0;

            int dif = cifraA - cifraB - imprumut;

            if (dif < 0)
            {
                dif += 10; 
                imprumut = 1;
            }
            else
            {
                imprumut = 0;
            }

            rez.Add(dif);
        }
        while (rez.Count > 1 && rez[rez.Count - 1] == 0)
        {
            rez.RemoveAt(rez.Count - 1);
        }

        return rez;
    }
    static List<int> Inmultire(List<int> a, List<int> b)
    {
        if ((a.Count == 1 && a[0] == 0) || (b.Count == 1 && b[0] == 0))
            return new List<int> { 0 };

        int lungimeMax = a.Count + b.Count;
        int[] tempRez = new int[lungimeMax];

        for (int i = 0; i < a.Count; i++)
        {
            for (int j = 0; j < b.Count; j++)
            {
                int produs = a[i] * b[j];
                tempRez[i + j] += produs;

                if (tempRez[i + j] >= 10)
                {
                    tempRez[i + j + 1] += tempRez[i + j] / 10;
                    tempRez[i + j] %= 10;
                }
            }
        }

        List<int> rez = tempRez.ToList();
        while (rez.Count > 1 && rez[rez.Count - 1] == 0)
        {
            rez.RemoveAt(rez.Count - 1);
        }

        return rez;
    }
}*/

