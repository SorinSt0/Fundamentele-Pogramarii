while (a3 < n1 && b3 < n2)
{
    if (v1[a3] == v2[b3])
    {
        a3++;
        b3++;
    }
    else if (v1[a3] < v2[b3])
    {
        difv1[k3] = v1[a3];
        a3++; k3++;
    }
    else
    {
        b3++;
    }   
}
while (a3 < n1)
{
    difv1[k3] = v1[a3];
    a3++; k3++;
}
for(int i = 0; i < k3; i++)
{
    Console.Write(difv1[i]+",");
}


int a4 = 0;
int b4 = 0;
int k4 = 0;
Console.WriteLine();
Console.Write("Diferenta dintre al doilea vector si primul vectore (v2-v1):  ");
int[] difv2 = new int[n2];
while (a4 < n1 && b4 < n2)
{
    if (v1[a4] == v2[b4])
    {
        a4++;
        b4++;
    }
    else if (v1[a4] < v2[b4])
    {
        a4++;
    }
    else
    {
        difv2[k4] = v2[b4];
        b4++;
        k4++;
    }    
}

while (b4 < n2)
{
    difv2[k4] = v2[b4];
    b4++; k4++;
}
