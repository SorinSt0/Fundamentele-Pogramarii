/*28)
class QuickSort{ 
static int partitei(int[] arr, int min, int max)
{
    int pivot = max;
    int i = min - 1;
    for (int j = min; j < max; j++)
    {
        if (arr[j]<pivot)
        {
            i++;
            swap(arr, i, j);
        }
    }
    swap(arr, i + 1, max);
    return i + 1;
}
static void swap(int[] arr ,int i,int j)
{
    int tempor = arr[i];
    arr[i]=arr[j];
    arr[j]=tempor;
}

static void quicksort(int[] arr, int min, int max)
{
    
    if (min < max)
    {
        int pi=partitei(arr, min, max);
        quicksort(arr, min, pi - 1);
        quicksort(arr, pi + 1, max);
    }
}
static void Main(string[] args)
{
        Console.WriteLine("Cate elemnte sa aiba vectorul: ");
        int n= Convert.ToInt32(Console.ReadLine());
        int[] arr = new int[n];
        Console.WriteLine("introduceti vectorii:");
        for ( int i = 0; i < arr.Length; i++)
        {
            arr[i]=Convert.ToInt32(Console.ReadLine());
        }

        quicksort(arr, 0, n - 1);
        Console.WriteLine("Vectorul sortat este:");
        for (int i = 0; i < arr.Length; i++)
        {
            Console.WriteLine(arr[i]);
        }    
}
}*/


/*29)
class MergeSort
{
    static void merge(int[] vect, int s, int d, int m)
    {
        int n1 = m - s + 1;
        int n2 = d - m;
        int[] stanga = new int[n1];
        int[] dreapta = new int[n2];
        int i, j;
        for (i = 0; i < n1; i++)
        {
            stanga[i] = vect[i];
        }
        for (j = 0; j < n2; j++)
        {
            dreapta[j] = vect[m + 1 + j];
        }

        i = 0; j = 0;
        int k = 0;
        while (i < n1 && j < n2)
        {
            if (stanga[i] < dreapta[j])
            {
                vect[k] = stanga[i];
                i++; k++;
            }
            else
            {
                vect[k] = dreapta[j];
                j++; k++;
            }
        }
        while (i < n1)
        {
            vect[k] = stanga[i];
            i++; k++;
        }
        while (j < n2)
        {
            vect[k] = dreapta[j];
            j++; k++;
        }
    }
    static void mergeSort(int[] vect, int s, int d)
    {
        if (s < d)
        {
            int m = s + (d - s) / 2;
            mergeSort(vect, s, m);
            mergeSort(vect, m + 1, d);
            merge(vect, s, d, m);
        }
    }
    static void Main(string[] args)
    {
        Console.WriteLine("Cate elemnte sa aiba vectorul: ");
        int n = Convert.ToInt32(Console.ReadLine());
        int[] vect = new int[n];
        Console.WriteLine("introduceti vectorii:");
        for (int i = 0; i < vect.Length; i++)
        {
            vect[i] = Convert.ToInt32(Console.ReadLine());
        }
        mergeSort(vect, 0, n - 1);
        Console.WriteLine("Vectorul sortat este:");
        for (int i = 0; i < vect.Length; i++)
        {
            Console.WriteLine(vect[i]);
        }
    }
}*/

